/dts-v1/;

#include "rk3399-excavator-sapphire.dtsi"
#include "rk3399-android.dtsi"
#include "rk3399-vop-clk-set.dtsi"

/ {
	model = "Rockchip RK3399 Excavator Board edp (Android)";
	compatible = "rockchip,android", "rockchip,rk3399-excavator-edp", "rockchip,rk3399";

	vcc_lcd: vcc-lcd {
		compatible = "regulator-fixed";
		regulator-name = "vcc_lcd";
		gpio = <&gpio4 22 GPIO_ACTIVE_HIGH>; // GPIO_C6
		startup-delay-us = <20000>;
		enable-active-high;
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		regulator-boot-on;
		vin-supply = <&vcc5v0_sys>;
	};

	panel: panel {
		compatible = "simple-panel";
		backlight = <&backlight>;
		power-supply = <&vcc_lcd>;
		enable-gpios = <&gpio1 13 GPIO_ACTIVE_HIGH>;
		prepare-delay-ms = <20>;
		enable-delay-ms = <20>;
		delay,prepare = <20>;
		delay,enable = <20>;

		display-timings {
			native-mode = <&timing0>;

			timing0: timing0 { // Innolux N116HSE-EBC eDP 1920x1080
				clock-frequency = <138780000>;
				hactive = <1920>;
				vactive = <1080>;
				hfront-porch = <48>;
				hsync-len = <32>;
				hback-porch = <80>;
				vfront-porch = <3>;
				vsync-len = <5>;
				vback-porch = <24>;
				hsync-active = <0>;
				vsync-active = <0>;
				de-active = <0>;
				pixelclk-active = <0>;
			};
		};

		ports {
			panel_in: endpoint {
				remote-endpoint = <&edp_out>;
			};
		};
	};

	test-power {
		status = "okay";
	};

	rt5651-sound {
		status = "disabled";
	};

	rt5640-sound {
		status = "okay";
	};

	hdmiin-sound {
		compatible = "rockchip,rockchip-rt5651-tc358749x-sound";
		rockchip,cpu = <&i2s0>;
		rockchip,codec = <&rt5651 &rt5651 &tc358749x>;
		status = "okay";
	};

	vcc5v0_hdmi: vcc5v0-hdmi-regulator {
		compatible = "regulator-fixed";
		enable-active-high;
		gpio = <&gpio4 27 GPIO_ACTIVE_HIGH>;
		pinctrl-names = "default";
		pinctrl-0 = <&hdmi_en>;
		regulator-name = "vcc5v0_hdmi";
		regulator-always-on;
	};

	vcc3v3_usbhub: vcc3v3-usbhub-regulator {
		compatible = "regulator-fixed";
		enable-active-high;
		gpio = <&gpio4 29 GPIO_ACTIVE_HIGH>;
		pinctrl-names = "default";
		pinctrl-0 = <&usb_hub_en>;
		regulator-name = "vcc3v3_usbhub";
		regulator-always-on;
	};

	usb-modem-card {
		compatible = "usb-modem-card";
		pinctrl-names = "default";
		pinctrl-0 = <&usb_modem_card_en>;
		enable-gpios = <&gpio0 6 GPIO_ACTIVE_HIGH>;
	};
};

&backlight {
	status = "okay";
	enable-gpios = <&gpio4 28 GPIO_ACTIVE_HIGH>; // GPIO_D4
};

&edp {
	status = "okay";
	force-hpd;

	ports {
		port@1 {
			reg = <1>;

			edp_out: endpoint {
				remote-endpoint = <&panel_in>;
			};
		};
	};
};

&edp_in_vopb {
	status = "disabled";
};

&edp_in_vopl {
	status = "okay";
};

&hdmi {
	status = "okay";
};

&hdmi_in_vopb {
	status = "okay";
};

&hdmi_in_vopl {
	status = "disabled";
};

&rt5651 {
	status = "disabled";
};

&cdn_dp {
	status = "okay";
	extcon = <&fusb0>;
	phys = <&tcphy0_dp>;
};

&hdmi_dp_sound {
	status = "okay";
};

&dp_in_vopb {
	status = "disabled";
};

&i2s0 {
	status = "okay";
};

&i2s1 {
	status = "okay";
};

&i2s2 {
	#sound-dai-cells = <0>;
	dmas = <&dmac_bus 4>;
	dma-names = "tx";
	status = "okay";
};

&i2c1 {
	status = "okay";

	gsl3673: gsl3673@40 {
		compatible = "GSL,GSL3673";
		reg = <0x40>;
		screen_max_x = <1536>;
		screen_max_y = <2048>;
		irq_gpio_number = <&gpio1 20 IRQ_TYPE_LEVEL_LOW>;
		rst_gpio_number = <&gpio4 22 GPIO_ACTIVE_HIGH>;
		status = "disabled";
	};

	tc358749x: tc358749x@0f {
		compatible = "toshiba,tc358749x";
		reg = <0x0f>;
		power-gpios = <&gpio2 6 GPIO_ACTIVE_HIGH>;
		power18-gpios = <&gpio2 9 GPIO_ACTIVE_HIGH>;
		power33-gpios = <&gpio2 5 GPIO_ACTIVE_HIGH>;
		csi-ctl-gpios = <&gpio2 10 GPIO_ACTIVE_HIGH>;
		stanby-gpios = <&gpio2 8 GPIO_ACTIVE_HIGH>;
		reset-gpios = <&gpio2 7 GPIO_ACTIVE_HIGH>;
		int-gpios = <&gpio2 12 GPIO_ACTIVE_HIGH>;
		pinctrl-names = "default";
		pinctrl-0 = <&hdmiin_gpios>;
		status = "disabled";
	};
};

&i2c6 {
	status = "okay";

	cw2015@62 {
		status = "disabled";
		compatible = "cw201x";
		reg = <0x62>;
		bat_config_info = <0x15 0x42 0x60 0x59 0x52 0x58 0x4D 0x48
				   0x48 0x44 0x44 0x46 0x49 0x48 0x32 0x24
				   0x20 0x17 0x13 0x0F 0x19 0x3E 0x51 0x45
				   0x08 0x76 0x0B 0x85 0x0E 0x1C 0x2E 0x3E
				   0x4D 0x52 0x52 0x57 0x3D 0x1B 0x6A 0x2D
				   0x25 0x43 0x52 0x87 0x8F 0x91 0x94 0x52
				   0x82 0x8C 0x92 0x96 0xFF 0x7B 0xBB 0xCB
				   0x2F 0x7D 0x72 0xA5 0xB5 0xC1 0x46 0xAE>;
		monitor_sec = <5>;
		virtual_power = <0>;
	};

	pn544: pn544@28 {
		compatible = "nxp,pn544";
		reg = <0x28>;
		clock-frequency = <400000>;
		interrupt-gpios = <&gpio2 7 GPIO_ACTIVE_HIGH>;
		enable-gpios = <&gpio2 8 GPIO_ACTIVE_HIGH>;
		firmware-gpios = <&gpio2 6 GPIO_ACTIVE_HIGH>;
		pinctrl-names = "default";
		pinctrl-0 = <&nfc_gpios>;
		status = "okay";
	};
};

&i2c7 {
	status = "disabled";
};

&spi1 {
	status = "disabled";
};

&uart4 {
	current-speed = <9600>;
	no-loopback-test;
	status = "okay";
};

&isp0 {
	status = "okay";
};

&isp1 {
	status = "okay";
};

&isp0_mmu {
	status = "okay";
};

&isp1_mmu {
	status = "okay";
};

&vopb {
	assigned-clocks = <&cru DCLK_VOP0_DIV>;
	assigned-clock-parents = <&cru PLL_VPLL>;
};

&vopl {
	assigned-clocks = <&cru DCLK_VOP1_DIV>;
	assigned-clock-parents = <&cru PLL_CPLL>;
};

&pcie_phy {
	status = "okay";
};

&pcie0 {
	ep-gpios = <&gpio2 4 GPIO_ACTIVE_HIGH>;
	num-lanes = <4>;
	pinctrl-names = "default";
	pinctrl-0 = <&pcie_clkreqn>;
	status = "okay";
};

&route_edp {
	status = "okay";
	logo,mode = "center";
	connect = <&vopl_out_edp>;
};

&route_hdmi {
	status = "okay";
	logo,mode = "center";
	connect = <&vopb_out_hdmi>;
};

&sdmmc {
	sd-uhs-sdr104;
	enable-gpios = <&gpio0 1 GPIO_ACTIVE_HIGH>;
};

&io_domains {
	status = "okay";
	bt656-supply = <&vcc1v8_dvp>; /* APIO2_VDD */
};

&pmu_io_domains {
	status = "okay";
	pmu1830-supply = <&vcc_1v8>; /* PMUIO2_VDD */
};

&pinctrl {
	lcd-panel {
		lcd_panel_reset: lcd-panel-reset {
			rockchip,pins = <4 30 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};

	hdmiin {
		hdmiin_gpios: hdmiin_gpios {
		rockchip,pins =
				<2 5 RK_FUNC_GPIO &pcfg_pull_none>,
				<2 6 RK_FUNC_GPIO &pcfg_pull_none>,
				<2 7 RK_FUNC_GPIO &pcfg_pull_none>,
				<2 8 RK_FUNC_GPIO &pcfg_pull_none>,
				<2 9 RK_FUNC_GPIO &pcfg_pull_none>,
				<2 12 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	hdmien {
		hdmi_en: hdmi-en {
			rockchip,pins = <4 27 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};

	usbhuben {
		usb_hub_en: usb-hub-en {
			rockchip,pins = <4 29 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};

	usb-modem-card {
		usb_modem_card_en: usb-modem-card-en {
			rockchip,pins = <0 6 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	sdmmc {
		sdmmc_en: sdmmc-en {
			rockchip,pins = <0 1 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	nfc {
		nfc_gpios: nfc-gpios {
			rockchip,pins =
					<2 6 RK_FUNC_GPIO &pcfg_pull_none>,
					<2 7 RK_FUNC_GPIO &pcfg_pull_none>,
					<2 8 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};
};

